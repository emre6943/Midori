<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainAnchor" prefHeight="550.0" prefWidth="440.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.Controller">
    <children>
        <VBox fx:id="main_vbox" prefHeight="550.0" prefWidth="440.0" />
        <JFXDrawer fx:id="drawer" defaultDrawerSize="205.0" layoutX="-3.0" prefHeight="550.0" prefWidth="205.0" />
        <JFXHamburger fx:id="hamburger" layoutX="14.0" layoutY="14.0" onMouseClicked="#openDrawer" prefHeight="40.0" prefWidth="40.0" style="-fx-background-color: #9dff15;" stylesheets="@stylesheet.css" />
        <Label fx:id="closeButton" layoutX="414.0" onMouseClicked="#closeWindow" stylesheets="@stylesheet.css" text="x" textFill="#9dff15">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <Label fx:id="minButton" layoutX="387.0" layoutY="-4.0" onMouseClicked="#minimizeWindow" stylesheets="@stylesheet.css" text="_" textFill="#9dff15">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </Label>
      <AnchorPane layoutX="30.0" layoutY="45.0" prefHeight="480.0" prefWidth="380.0" />
    </children>
</AnchorPane>
